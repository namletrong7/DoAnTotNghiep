{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd android && ./gradlew clean && cd ../ && react-native run-android",
    "ios": "cd ios && xcodebuild clean && cd ../ && react-native run-ios --simulator 'iPhone 14'",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "newclear": "rm -rf $TMPDIR/react-* && rm -rf /tmp/metro-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i --force",
    "bundle": "npx jetify && cd android && ./gradlew bundleRelease && cd ../",
    "release": "npx jetify && cd android && ./gradlew app:assembleRelease && cd ../",
    "release_and_install": "npx jetify && cd android && ./gradlew app:assembleRelease && cd app/build/outputs/apk/release/; adb install -r app-release.apk &&  cd ../ &&  cd ../ &&  cd ../ &&  cd ../ &&  cd ../",
    "start": "react-native start",
    "start1": "export NODE_OPTIONS=--openssl-legacy-provider && react-native start",
    "auto_fix_lint": "npx eslint --fix .",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "reset_assets": "rm -rf ios/assets && npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "pod:install": "cd ios && pod install && cd ..",
    "pod:update": "cd ios && pod update && cd ..",
    "pod:update1": "cd ios && pod install --repo-update && cd ..",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.1",
    "@likashefqet/react-native-image-zoom": "^3.0.0",
